<h2 style="font-size: 40px; margin-bottom: 40px; color: #f70b0b">
  <span style="color: #1ab394; font-weight: 900">Infotech</span>
</h2>
<!--<div class="ibox-content"> -->
<div id="wrapper">
  <div class="form-group m-t">
    <%= form_with(model: @company, local: true) do |f| %>
    <div class="form-group">
      <label>Company Name</label>
      <%= f.text_field :name, :placeholder=>"Enter Company Name",
      :class=>"form-control" %>
    </div>
  </div>
  <%= f.submit 'Submit', :class => 'btn btn-sm btn-primary pull-right m-t-n-xs'
  %> <%= link_to 'Back', companies_path, :class=>"btn btn-sm pull-left
  btn-primary m-t-n-xs" %> <% end %>
</div>
