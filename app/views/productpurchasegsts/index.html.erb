        <td><%= productpurchasegst.rkpurchasesubtotal %></td>
        <td><%= productpurchasegst.rkpurchasetotaligst %></td>
        <td><%= productpurchasegst.rkpurchasetotalcgst %></td>
        <td><%= productpurchasegst.rkpurchasetotalsgst %></td>
        <td><%= productpurchasegst.rkpurchasediscount %></td>
        <td><%= productpurchasegst.rkpurchaseshipping %></td>
        <td><%= productpurchasegst.rkpurchasegrandtotal %></td>
        <td><%= productpurchasegst.rkpurchaseinwords %></td>
        <td><%= productpurchasegst.rkpurchaseunittotal %></td>
        <td><%= link_to 'Show', productpurchasegst %></td>
        <td><%= link_to 'Edit', edit_productpurchasegst_path(productpurchasegst) %></td>
        <td><%= link_to 'Destroy', productpurchasegst, method: :delete, data: { confirm: 'Are you sure?' } %></td>
