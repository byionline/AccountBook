  <tbody>
    <% @productpurchasegsts.each do |productpurchasegst| %>
      <tr>
        <td><%= productpurchasegst.chartaccount %></td>
        <td><%= productpurchasegst.supplier %></td>
        <td><%= productpurchasegst.employee %></td>
        <td><%= productpurchasegst.rkpurchaseinvoiceno %></td>
        <td><%= productpurchasegst.rkpurchasedate %></td>
        <td><%= productpurchasegst.rkpurchaseduedate %></td>
